{
  "moduleContent": {
      "$edgeAgent": {
          "properties.desired": {
              "schemaVersion": "1.0",
              "runtime": {
                  "type": "docker",
                  "settings": {
                      "minDockerVersion": "v1.25",
                      "loggingOptions": ""
                  }
              },
              "systemModules": {
                  "edgeAgent": {
                      "type": "docker",
                      "settings": {
                          "image": "microsoft/azureiotedge-agent:1.0-preview",
                          "createOptions": "{}"
                      }
                  },
                  "edgeHub": {
                      "type": "docker",
                      "status": "running",
                      "restartPolicy": "always",
                      "settings": {
                          "image": "microsoft/azureiotedge-hub:1.0-preview",
                          "createOptions": "{\"HostConfig\":{\"PortBindings\":{\"8883/tcp\":[{\"HostPort\":\"8883\"}],\"443/tcp\":[{\"HostPort\":\"443\"}]}}}"
                      }
                  }
              },
              "modules": {
                  "accelSim": {
                      "version": "1.0",
                      "type": "docker",
                      "status": "running",
                      "restartPolicy": "always",
                      "settings": {
                          "image": "ndccontosoreg.azurecr.io/accel_sim:0.0.1-amd64",
                          "createOptions": "{}"
                      }
                  }
              }
          }
      },
      "$edgeHub": {
          "properties.desired": {
              "schemaVersion": "1.0",
              "routes": {
                  "route": "FROM /* INTO $upstream"
              },
              "storeAndForwardConfiguration": {
                  "timeToLiveSecs": 7200
              }
          }
      },
      "accelSim": {
          "properties.desired": {
              "fall": true
          }
      }
  }
}